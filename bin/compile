#!/bin/sh
mkdir /app/yt
echo "curl -L https://yt-dl.org/downloads/latest/youtube-dl -o /app/yt/youtube-dl"
curl -L https://yt-dl.org/downloads/latest/youtube-dl -o /app/yt/youtube-dl
echo "chmod a+rx /app/yt/youtube-dl"
chmod a+rx /app/yt/youtube-dl
pip3 install --upgrade youtube_dl
pip3 install --upgrade pip aria2p cryptg dialog-py dialogs ffmpeg-python gitpython heroku heroku3 requests tgcrypto pillow pydownload pyfiglet utilities-package python-git pytube  python-sql pythondialog redis regex requests selenium setuptools sql sqlalchemy telegraph telethon telethon-session-sqlalchemy lxml pysmartdl python-dateutil tgcrypto pygithub
